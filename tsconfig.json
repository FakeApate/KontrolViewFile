{
    "include": ["./src/**/*"],
    "exclude": ["./build/**/*"],
    "compilerOptions": {
        "outDir": "./build",
        "allowJs": false,
        "target": "ES6",
        "moduleResolution": "nodenext",
        "allowSyntheticDefaultImports": true,
        "strictNullChecks": true,
    }
    
}